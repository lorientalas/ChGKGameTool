<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ChGKGameTool.Styles">
    <Color x:Key="BackgroundLightOrange">#FFDA932D</Color>
    <SolidColorBrush x:Key="BackgroundLightOrangeBrush" Color="{StaticResource BackgroundLightOrange}" />
    <Color x:Key="BackgroundOrange">#FFF07B38</Color>
    <SolidColorBrush x:Key="BackgroundOrangeBrush" Color="{StaticResource BackgroundOrange}" />
    <Color x:Key="BackgroundAlmostBlack">#FF333333</Color>
    <SolidColorBrush x:Key="BackgroundAlmostBlackBrush" Color="{StaticResource BackgroundAlmostBlack}" />
    <Color x:Key="BackgroundLightBlack">#FF444444</Color>
    <SolidColorBrush x:Key="BackgroundLightBlackBrush" Color="{StaticResource BackgroundLightBlack}" />
    <Color x:Key="ForegroundGray">#FF999999</Color>
    <SolidColorBrush x:Key="ForegroundGrayBrush" Color="{StaticResource ForegroundGray}" />

    <Color x:Key="ForegroundOrange">#FFF07B38</Color>
    <SolidColorBrush x:Key="ForegroundOrangeBrush" Color="{StaticResource ForegroundOrange}" />
</ResourceDictionary>